{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "/css/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/js/$1"
    },
    {
      "src": "/fonts/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/fonts/$1"
    },
    {
      "src": "/(female|male|s|message|gallery)\\.html",
      "dest": "/$1.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/assets/*"],
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "cache-control": "no-cache",
        "service-worker-allowed": "/"
      },
      "dest": "/sw.js"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 